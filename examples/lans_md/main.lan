PART main
    RULE md         $Blocks
END

PART Block
    RULE qoute      <-< *mdquote >->
    IF :0 0
        RULE style  *Effect $Blocks( @0:0 ) *Effect[:0=@0:0]
        RULE text   *TREffect
    END
    UNLESS :0 0
        RULE style  *Effect[:0!=:0] *TREffect[:0=@0:0] *Effect[:0=@0:0]
        RULE text   *TREffect
    END
END

PART Blocks
    RULE r          $Block( :0 ) ?$Blocks
END